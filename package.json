{
  "name": "blog-api-server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jagrajan Bhullar <jag@jagrajan.com>",
  "license": "MIT",
  "scripts": {
    "init": "npm-run-all init:*",
    "init:create": "node -r dotenv/config init/create-db.js",
    "init:populate": "babel-node init/populate-db.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build": "webpack --config webpack.dev.js --watch",
    "dev:server": "nodemon --watch build --exec \"node -r dotenv/config build/bundle.js\""
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "async-redis": "^1.1.7",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "credential": "^2.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.1.4",
    "express-fingerprint": "^1.1.2",
    "image-data-uri": "^2.0.0",
    "node-cache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.8.1",
    "pug": "^2.0.4",
    "simple-node-logger": "^18.12.22",
    "slugify": "^1.3.4",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "source-map-support": "^0.5.10"
  }
}
